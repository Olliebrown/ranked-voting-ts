{
  "name": "ranked-voting",
  "version": "0.0.2",
  "description": "Ranked choice voting library",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikey-t/ranked-voting-ts.git"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.js",
  "scripts": {
    "build": "gulp build",
    "example": "node dist/src/example.js",
    "watch": "tsc --watch",
    "test": "mocha || exit 0",
    "test:coverage": "nyc mocha || exit 0",
    "clean": "gulp clean",
    "pack": "gulp pack",
    "tsc": "tsc",
    "prepack": "gulp build",
    "prepublish": "gulp build"
  },
  "keywords": [],
  "author": "Mike Thompson",
  "license": "MIT",
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test",
      "coverage",
      "src/index.ts",
      "src/example.ts",
      "gulpfile.js"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.1",
    "chai": "^4.3.4",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "mocha": "^9.1.2",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@mikeyt23/node-cli-utils": "^1.0.0"
  }
}
